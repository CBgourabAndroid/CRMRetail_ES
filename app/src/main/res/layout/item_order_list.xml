<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#80ffffff">


    <LinearLayout
        android:id="@+id/itemlay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <!-- <androidx.cardview.widget.CardView

                 android:id="@+id/ghl"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:layout_gravity="center"
                 android:elevation="6dp"
                 card_view:cardBackgroundColor="@android:color/white"
                 card_view:cardCornerRadius="5dp">-->

            <ImageView
                android:id="@+id/ivFish"
                android:layout_width="40dp"
                android:layout_gravity="center"
                android:layout_height="40dp"
                android:visibility="gone"
                />


            <!--</androidx.cardview.widget.CardView>-->


        </LinearLayout>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/datetime"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:gravity="right|bottom"
                android:text="2020-07-17 15:29:34"
                android:textSize="10dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">
                    <TextView

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Products &amp; Quantity"
                        android:textSize="16dp"
                        android:textColor="@color/colorAccent"
                        android:layout_marginLeft="5dp"
                        />

                    <TextView
                        android:id="@+id/textFishName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:text=""
                        android:layout_alignParentLeft="true"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textSize="13dp"
                        />

                </LinearLayout>



                <TextView
                    android:id="@+id/pricetxt"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="â‚¹ 10.000"
                    android:gravity="right"
                    android:layout_toRightOf="@+id/textFishName"
                    android:textStyle="bold"
                    android:layout_toLeftOf="@+id/statustxt"
                    android:textSize="13dp"
                    android:textColor="@color/black"
                    />
                <TextView
                    android:id="@+id/statustxt"
                    android:layout_width="0dp"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_gravity="right"
                    android:layout_alignParentRight="true"
                    android:text=""
                    android:gravity="center"
                    android:textColor="@color/white"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:textStyle="bold" />

            </LinearLayout>

            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Quantity"
                android:textStyle="bold"
                android:visibility="gone"
                android:textSize="16dp"
                android:textColor="@color/colorAccent"
                android:layout_marginLeft="5dp"
                />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3.00, 10.00"
                android:textSize="15dp"
                android:visibility="gone"
                android:layout_marginLeft="5dp"
                android:textAppearance="?android:attr/textAppearanceMedium"/>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">



                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Dealer Approved"
                    android:textSize="16dp"
                    android:textColor="@color/colorPrimaryDark"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:id="@+id/dlrap"
                    android:layout_width="wrap_content"
                    android:layout_height="21dp"
                    android:gravity="right"
                    android:text="NO"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:textSize="15dp" />


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">



                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Organization Approved"
                    android:textSize="16dp"
                    android:textColor="@color/logoColorTwo"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:id="@+id/orgapv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NO"
                    android:gravity="right"
                    android:textSize="15dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"/>


            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">



                <TextView

                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:text="Received"
                    android:textSize="16dp"
                    android:textColor="@color/lightblue"
                    android:layout_marginLeft="5dp"
                    />

                <TextView
                    android:id="@+id/rcvsts"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="NO"
                    android:gravity="right"
                    android:textSize="15dp"
                    android:textAppearance="?android:attr/textAppearanceMedium"/>


            </LinearLayout>
        </LinearLayout>







    </LinearLayout>


    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#1A000000"
        android:layout_marginRight="15dp"
        android:layout_marginLeft="67dp"/>

</LinearLayout>